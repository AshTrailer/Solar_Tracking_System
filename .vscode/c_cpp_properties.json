{
  "configurations": [
    {
      "name": "ESP-IDF",
      "compilerPath": "${config:idf.toolsPath}/tools/riscv32-esp-elf/esp-14.2.0_20241119/riscv32-esp-elf/bin/riscv32-esp-elf-gcc",
      "compileCommands": "${config:idf.buildPath}/compile_commands.json",
      "includePath": [
        "${workspaceFolder}/**",
        "${config:idf.espIdfPath}/components/**",
        "${config:idf.espIdfPath}/components/bt/include/esp32c6/include"
      ],
      "defines": [
        "CONFIG_IDF_TARGET_ESP32C6=1"
      ],
      "browse": {
        "path": [
          "${workspaceFolder}",
          "${config:idf.espIdfPath}/components/**",
          "${config:idf.espIdfPath}/components/bt/include/esp32c6/include"
        ],
        "limitSymbolsToIncludedHeaders": true
      }
    }
  ],
  "version": 4
}
